#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    char ch ;
    scanf("%c",&ch);
    switch(ch)
        {
            case 'a':
            case 'e':
            case 'i':
            case 'o':
            case 'u':
            case 'A':
            case 'E':
            case 'I':
            case 'O':
            case 'U':
            printf("The Character %c is Vowel",ch);
            break;
            default:
              printf("The Character %c is Consonant",ch);
            }
    return 0;
}
