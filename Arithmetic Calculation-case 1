#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int x,y;
    char op;
    scanf("%d",&x);
    scanf("%d",&y);
    scanf(" %c",&op);
    switch(op){
        case '+':
        printf("Addition of two number is %.1f\n",(float)(x+y));
        break;
     case '-':
        printf("Subtraction of two number is %.1f\n",(float)(x-y));
        break;
     case '*':
        printf("Multiplication of two number is %.1f\n",(float)(x*y));
        break;
     case '/':
      if(y!=0)
        printf("Division of two number is %.1f\n",(float)(x/y));
       else
         printf("Invalid Input\n");
        break;
     case '%':
      if(y!=0)
        printf("Modulo of two number is %.1fn",(float)(x%y));
       else
         printf("Invalid Input\n");
        break;
    default:
    printf("Invalid Input\n");
     }
return 0;
}
